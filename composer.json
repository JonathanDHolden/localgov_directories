{
    "name": "localgovdrupal/localgov_directories",
    "description": "LocalGovDrupal distribution: Directories feature.",
    "type": "drupal-module",
    "homepage": "https://github.com/localgovdrupal/localgov_directories",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "require": {
        "drupal/facets": "^2.0",
        "drupal/pathauto": "^1.6",
        "drupal/search_api": "^1.17",
        "drupal/search_api_autocomplete": "^1.3",
        "drupal/search_api_location": "1.x-dev",
        "localgovdrupal/localgov_core": "^2.1",
        "localgovdrupal/localgov_geo": "^1.1"
    },
    "require-dev": {
        "localgovdrupal/localgov_services": "^2.0",
        "localgovdrupal/localgov_openreferral": "^1.0"
    },
    "suggest": {
        "localgovdrupal/localgov_openreferral": "Enables Open Referral output of Directories"
    },
    "extra": {
        "patches": {
            "drupal/search_api": {
                "Location search for Search API Database backend": "https://www.drupal.org/files/issues/2022-07-12/2877319-30-db_backend_location_support.patch"
            },
            "drupal/search_api_location": {
                "Geocoder selection plus Coordinate extraction bug": "https://www.drupal.org/files/issues/2022-06-03/3283994-3_0.patch"
            }
        }
    }
}
